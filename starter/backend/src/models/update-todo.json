{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "update-todo",
    "type": "object",
    "properties": {
      "name": {
        "type": "string",
        "pattern": "^[a-zA-Z0-9]+$",
        "minLength": 5,
        "maxLength": 20
      },
      "dueDate": {
        "type": "string"
      },
      "done": {
        "type": "boolean"
      }
    },
    "required": ["name", "dueDate"],
    "additionalProperties": true
  }